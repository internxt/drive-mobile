{
  "name": "drive-mobile",
  "displayName": "drive-mobile",
  "expo": {
    "name": "drive-mobile",
    "slug": "drive-mobile",
    "version": "1.0.0",
    "sdkVersion": "40.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.internxt.snacks",
      "usesIcloudStorage": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.internxt.cloud",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "inxt"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "inxt"
  }
}
